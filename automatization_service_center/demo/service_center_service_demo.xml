<odoo>
    <data noupdate="1">

        <record id="service_center_change_wheel" model="service.center.service">
            <field name="name">Change Wheel</field>
            <field name="duration">0.25</field>
            <field name="price">25</field>
            <field name="currency_id" ref="base.USD"/>
        </record>

        <record id="service_center_windshield_replacement" model="service.center.service">
            <field name="name">Windshield Replacement</field>
            <field name="duration">1.50</field>
            <field name="price">500</field>
            <field name="currency_id" ref="base.USD"/>
        </record>

        <record id="service_center_change_2_wheels" model="service.center.service">
            <field name="name">Change 2 wheels</field>
            <field name="duration">0.50</field>
            <field name="price">50</field>
            <field name="currency_id" ref="base.USD"/>
        </record>
    </data>
</odoo>
